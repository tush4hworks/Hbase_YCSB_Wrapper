{
  "wrap": {
    "cluster":{
      "host":"localhost",
      "clustername":"DPH",
      "user":"admin",
      "password":"admin"
      },
    "zeppelin":"false",
    "notebook":{
      "host":"localhost:9995",
      "user":"user1",
      "password":"user1",
      "note":"300GBSparkSql",
      "zepInputFile":"zepin.csv"
      },
      "ambariMetrics":{
    "collector":{
      "host":{"metrics":"mem_free,disk_free,cpu_system,cpu_user,bytes_in,bytes_out,yarn.NodeManagerMetrics.ContainersRunning,yarn.QueueMetrics.Queue=root.AllocatedMB,yarn.QueueMetrics.Queue=root.AllocatedVCore",
              "hostname":"dphslave%","dumpfile":"host_stats.csv"},
      "service":{"metrics":"yarn.NodeManagerMetrics.ContainersRunning._SUM,yarn.QueueMetrics.Queue=root.AllocatedMB._SUM,yarn.QueueMetrics.Queue=root.AllocatedVCore._SUM"
                ,"dumpfile":"service_stats.csv"}
    },
      "metricsHost":"localhost",
      "metricsPort":"6188"
    },
    "numRuns": 1,
    "printer":["hbase-site"],
    "enableRollBack":"true",
    "rollBackService":"HBASE",
    "base_version":1,
    "workloads":["workloada","workloadb"],
    "settings": [
      {
        "config": {
          "ambari": {
            "hbase-site": {
              "hbase.hregion.memstore.block.multiplier": "4"
            }
          },
          "runconf":{
              "records":"300000000",
              "operations":"10000000",
              "runthreads":"400",
              "loadthreads":"40"
            },
          "system":["cp /etc/hbase/conf/hbase-site.xml hbase10/conf"],
          "restart": {
            "components": [
            ],
            "services": ["HBASE"]
          }
        },
        "name": "4XBeforeFlush"
      },
      {
        "config": {
          "ambari": {
            "hbase-site": {
              "hbase.hregion.memstore.block.multiplier": "8"
            }
          },
          "system":["cp /etc/hbase/conf/hbase-site.xml hbase10/conf"],
          "runconf":{
            "records":"300000000",
              "operations":"10000000",
              "runthreads":"400",
              "loadthreads":"40"
            },
          "restart": {
            "components": [
            ],
            "services": ["HBASE"]
          }
        },
        "name": "8XBeforeFlush"
      }
    ]
  }
}